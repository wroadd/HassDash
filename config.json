{
    "name": "HassDash",
    "version": "0.0.1",
    "slug": "aHassDash",
    "description": "Master version of HADaemon with HADashboard (https://github.com/home-assistant/appdaemon/tree/master)",
    "url": "https://github.com/wroadd/hassdash",
    "startup": "after",
    "image": "wroadd/hassdash",
    "boot": "auto",
    "map": ["config:rw"],
    "options": {
        "domain": "cp.local.io"
    },
    "schema": {
        "domain": "str"
    }, 
    "ports": {
        "80/tcp": 3031
    }
}
